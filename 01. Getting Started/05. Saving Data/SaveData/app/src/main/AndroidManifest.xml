<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.george.savedata">

    <!-- Obtain Permissions for External Storage -->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".SaveKeyValueSetsActivity"
            android:label="@string/key_value_label">
        </activity>
        <activity
            android:name=".LoadKeyValueSetsActivity"
            android:label="@string/key_value_load_label">
        </activity>
        <activity
            android:name=".SaveFilesActivity"
            android:label="@string/file_label">
        </activity>
    </application>

</manifest>